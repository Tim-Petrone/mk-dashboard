<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/service-definition-2.1.xsd">

    <service verb="get" noun="PartyName" type="java" location="com.mkdecision.dashboard.PartyServices" method="getPartyName">
        <in-parameters>
            <parameter name="partyId"/>
        </in-parameters>
        <out-parameters>
            <parameter name="partyId"/>
            <parameter name="partyName"/>
        </out-parameters>
    </service>
    <service verb="get" noun="PartyPrimaryTelecomNumber" type="java" location="com.mkdecision.dashboard.PartyServices" method="getPartyPrimaryTelecomNumber">
        <in-parameters>
            <parameter name="partyId"/>
        </in-parameters>
        <out-parameters>
            <parameter name="telecomNumber"/>
        </out-parameters>
    </service>
    <service verb="get" noun="PartyPrimaryEmailAddress" type="java" location="com.mkdecision.dashboard.PartyServices" method="getPartyPrimaryEmailAddress">
        <in-parameters>
            <parameter name="partyId"/>
        </in-parameters>
        <out-parameters>
            <parameter name="partyId"/>
            <parameter name="emailAddress"/>
        </out-parameters>
    </service>
    <service verb="get" noun="PartyPrimaryPostalAddress" type="java" location="com.mkdecision.dashboard.PartyServices" method="getPartyPrimaryPostalAddress">
        <in-parameters>
            <parameter name="partyId"/>
        </in-parameters>
        <out-parameters>
            <parameter name="partyId"/>
            <parameter name="postalAddress"/>
        </out-parameters>
    </service>
    <service verb="get" noun="PartyProductStoreRoles" type="java" location="com.mkdecision.dashboard.PartyServices" method="getPartyProductStoreRoles">
        <in-parameters>
            <parameter name="partyId"/>
            <parameter name="productStoreId"/>
        </in-parameters>
        <out-parameters>
            <parameter name="partyId"/>
            <parameter name="productStoreId"/>
            <parameter name="roleTypeIdSet" type="Set"/>
        </out-parameters>
    </service>

</services>