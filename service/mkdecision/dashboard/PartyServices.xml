<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/service-definition-2.1.xsd">

    <service verb="get" noun="PartyName" type="script" location="component://mk-dashboard/script/mkdecision/dashboard/getPartyName.groovy">
        <in-parameters>
            <parameter name="partyId"/>
        </in-parameters>
        <out-parameters>
            <parameter name="partyName"/>
        </out-parameters>
    </service>
    <service verb="get" noun="PartyPrimaryPhoneNumber" type="script" location="component://mk-dashboard/script/mkdecision/dashboard/getPartyPrimaryPhoneNumber.groovy">
        <in-parameters>
            <parameter name="partyId"/>
        </in-parameters>
        <out-parameters>
            <parameter name="telecomNumber"/>
        </out-parameters>
    </service>
    <service verb="get" noun="PartyPrimaryEmailAddress" type="script" location="component://mk-dashboard/script/mkdecision/dashboard/getPartyPrimaryEmailAddress.groovy">
        <in-parameters>
            <parameter name="partyId"/>
        </in-parameters>
        <out-parameters>
            <parameter name="emailAddress"/>
        </out-parameters>
    </service>

</services>